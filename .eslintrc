{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "rules": {
     "import/no-named-as-default": 0,
     "import/prefer-default-export": 0,
     "jsx-a11y/anchor-is-valid": [ "error", { "components": [ "Link" ], "specialLink": [ "to" ] } ]
  },
  "env": {
    "browser": true,
    "node": true
  }
}